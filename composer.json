{
	"name": "vasek-purchart/rabbit-mq-database-transaction-producer-bundle",
	"description": "Publish messages to RabbitMQ producer when database transaction was committed",
	"license": "MIT",
	"authors": [
		{
			"name": "Va≈°ek Purchart",
			"email": "me@vasekpurchart.cz",
			"homepage": "http://vasekpurchart.cz"
		}
	],
	"require": {
		"php": "~7.4 || ~8.0",
		"doctrine/doctrine-bundle": "~1.3|~2.0",
		"php-amqplib/rabbitmq-bundle": "~1.9",
		"psr/log": "~1.0",
		"symfony/config": "~3.0|~4.0|~5.0",
		"symfony/dependency-injection": "~3.2|~4.0|~5.0",
		"symfony/http-kernel": "~3.0|~4.0|~5.0",
		"symfony/yaml": "~3.0|~4.0|~5.0"
	},
	"require-dev": {
		"ext-pdo_sqlite": "*",
		"consistence-community/coding-standard": "3.11.0",
		"doctrine/common": "~3.0",
		"doctrine/dbal": "~3.0",
		"matthiasnoback/symfony-dependency-injection-test": "4.2.1",
		"php-parallel-lint/php-parallel-lint": "1.2.0",
		"phing/phing": "2.16.4",
		"phpunit/phpunit": "9.5.2"
	},
	"autoload": {
		"psr-4": {
			"VasekPurchart\\RabbitMqDatabaseTransactionProducerBundle\\": "src"
		},
		"classmap": [ "src" ]
	},
	"autoload-dev": {
		"psr-4": {
			"VasekPurchart\\RabbitMqDatabaseTransactionProducerBundle\\": "tests"
		},
		"classmap": [ "tests" ]
	},
	"config": {
		"bin-dir": "bin"
	}
}
